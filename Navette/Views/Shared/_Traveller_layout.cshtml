
@{
    ViewBag.Title = "_Login_layout_provider";
    Layout = "";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css_provider")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body class="container full-height-grow">
    <header class="main-header">


        <a class="brand-logo" href="/">
            <img id="icon" src="~/Content/Images/Logo_white.png" />
            @*            <div class="brand-logo-name">Navette</div>
            *@
        </a>

        <nav class="main-nav">
            <ul class="navbar-nav mr-auto">
                <li><a href=@Url.Action("ViewSubs","Traveller")>Subscriptions</a></li>
                <li><a href=@Url.Action("CustomRequest","Traveller")>Custom Request</a></li>
                <li><a href=@Url.Action("Logout","Traveller")>Logout</a></li>
            </ul>


        </nav>

    </header>
    <div class="container body-content">
        @RenderBody()
        @*   <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
            </footer>*@
    </div>
    <div class="home-page-circle-1"></div>
    <div class="home-page-circle-2"></div>
    <div class="home-page-circle-3"></div>
    <div class="home-page-circle-4"></div>
    @*    @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)*@
</body>
</html>
